{% extends 'partials/base.html.twig' %}

{% block main %}
    {% for module in page.collection() %}
        {% if module.header.position == 'main' %}
            {{ module.content | raw }}
        {% endif %}
    {% endfor %}

{% endblock %}

{% block aside %}

    {% for module in page.collection() %}
        {% if module.header.position == 'sidebar' %}
            {{ module.content | raw }}
        {% endif %}
    {% endfor %}

{% endblock %}
