{% extends 'partials/base.html.twig' %}

{% block main %}

  {% for module in page.collection() %}
      {% if module.header.position == 'main' or module.template() == 'modular/entry_list' %}
        {{ module.content }}
      {% endif %}
  {% endfor %}

{% endblock %}

{% block aside %}

    {% for module in page.collection() %}
        {% if module.header.position == 'sidebar' %}
            {{ module.content }}
        {% endif %}
    {% endfor %}

{% endblock %}
